AAA;AACA,oBAAA,KAAA,CAAA,KAAA,CAAA,kBAAA,EAAA,KAAA;;AACA,oBAAA,KAAA,CAAA,OAAA,GAAA,EAAA;;AACA,oBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AACA,mBAJA,EAIA,IAJA,CAAA;AAKA;;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,eAnBA,CALA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAbA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"modal\" id=\"exampleModalCenter\" v-if=\"status\">\r\n      <div class=\"modal-dialog col-md-5\" role=\"document\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"alert\" v-if=\"message != ''\">{{ message }}</div>\r\n          <form class=\"my-4\">\r\n            <div class=\"container\">\r\n              <div class=\"form-group\">\r\n                <label class=\"text-dark\">Email</label>\r\n                <input\r\n                  v-model=\"username\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"example@doamin.com\"\r\n                />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <label class=\"text-dark\">Şifre</label>\r\n                <input v-model=\"password\" type=\"text\" placeholder=\"******\" class=\"form-control\" />\r\n              </div>\r\n            </div>\r\n          </form>\r\n          <div class=\"modal-footer\">\r\n            <button\r\n              type=\"button\"\r\n              @click=\"loginModalClose\"\r\n              class=\"btn btn-outline-secondary btn-sm\"\r\n              data-dismiss=\"modal\"\r\n            >Kapat</button>\r\n            <button type=\"button\" class=\"btn btn-success btn-sm\" @click=\"login\">Giriş Yap</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: \"\",\r\n      password: \"\",\r\n      message: \"\"\r\n    };\r\n  },\r\n  props: {\r\n    status: {\r\n      required: true\r\n    }\r\n  },\r\n  methods: {\r\n    loginModalClose() {\r\n      this.$emit(\"closedLoginModal\", false);\r\n    },\r\n    async login() {\r\n      await this.$store\r\n        .dispatch(\"User/getUser\", {\r\n          username: this.username,\r\n          password: this.password\r\n        })\r\n        .then(res => {\r\n          if (res.error) {\r\n            this.message = res.error.message;\r\n          } else {\r\n            this.username = \"\";\r\n            this.password = \"\";\r\n            this.message = res.success.message;\r\n            setTimeout(() => {\r\n              this.$emit(\"closedLoginModal\", false);\r\n              this.message = \"\";\r\n              this.$router.push(\"/profile\");\r\n            }, 1500);\r\n          }\r\n          console.log(res);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style >\r\n.modal {\r\n  display: block !important;\r\n  background: rgba(0, 0, 0, 0.5);\r\n}\r\n.modal-dialog {\r\n  border: none !important;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}